{% extends '../layout.njk' %}

{% block content %}
   <section class='animation-delete'>
      <div class="delete-lottie"></div>
      <h2>Excluído com sucesso</h2>
	 <a href="/">Retornar a página principal</a>
   </section>

   <script src='/lottie.min.js'></script>
   <script>
      const element = document.querySelector('.delete-lottie')
      lottie.loadAnimation({
	 container: element,
	 render: 'svg',
	 loop: true,
	 autoplay: true,
	 path: '/lottie/44677-maintenance.json' 
      }).setSpeed(2.5)
   </script>

{% endblock content %}
